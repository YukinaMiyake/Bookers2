<%= render partial: 'books/list', locals: { user: @user, book: @book } %> 

<div class="col-md-8 offset-md-1">
  <h2>Users</h2>
  <table class="table">
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
      </tr>
    </thead>
  <tbody>
    <% @users.each do |users| %>
    <tr>
      <td><%= image_tag users.get_profile_image(100,100) %></td>
      <td><%= users.name %></td>
      <td><%= link_to 'Show', user_path(users.id) %></td>
    </tr>
    <% end %>
  </tbody>
  </table>
</div>

  </div>
</div>