<%= render partial: 'list', locals: { user: @user, book: @book } %> 

<h2>Book detail</h2>
<table>
  <tbody>
  <td><%= image_tag @user.get_profile_image(100,100) %><br />
      <p><%= link_to @user.name, user_path(@user.id) %></p></td>
  <td><%= @books.title %></td>
  <td><%= @books.body %></td>
  <td><%= link_to 'Edit', edit_book_path(@books.id) %></td>
  <td><%= link_to 'Destroy', books_path, method: :delete, "data-confirm" => "本当に消しますか？" %></td>
  </tbody>
</table>
